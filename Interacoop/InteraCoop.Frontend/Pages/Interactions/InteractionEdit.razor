@page "/interaction/edit/{InteractionId:int}"

@if (loading)
{
    <Loading />
}
else
{
    <InteractionForm @ref="interactionForm" 
        Interaction="interactionDto" 
        OnValidSubmit="SaveChangesAsync" 
        ReturnAction="Return" 
        FormName="Editar Interacción"
        CardName="Interacción"/>
   
}