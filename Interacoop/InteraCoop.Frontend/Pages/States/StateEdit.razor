@page "/states/edit/{StateId:int}"

@if (state is null)
{
    <Loading />
}
else
{
    <FormWithName @ref="stateForm" 
        Label="Departamento:" 
        Model="state" OnValidSubmit="SaveAsync" 
        ReturnAction="Return" 
        IconName="@Icons.Material.Filled.Place" 
        FormName="Editar departamento" />
}

