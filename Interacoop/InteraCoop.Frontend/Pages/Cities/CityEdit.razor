@page "/cities/edit/{CityId:int}"

@if (city is null)
{
    <Loading />
}
else
{
    <FormWithName @ref="cityForm" 
        Label="Ciudad:" Model="city" 
        OnValidSubmit="SaveAsync" 
        ReturnAction="Return" 
        IconName="@Icons.Material.Filled.Place"
        FormName="Editar ciudad" />
}
